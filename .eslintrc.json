{
  "extends": ["airbnb", "airbnb/hooks", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y", "emotion"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": ["**/*.test.js", "**/*.spec.js", "**/__mocks__/*.js", "**/setup-test-env.js", "**/jest-preprocess.js"]
    }]
  }
}
